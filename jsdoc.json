{
  "source": {
    "include": [
      "src"
    ],
    "includePattern": ".jsx$"
  },
  "opts": {
    "destination": "./docs/autogenerated",
    "recurse": true,
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown"
  ]
}
